{
    "dependencies": {
        "node-sass": "^4.13.1"
    },
    "scripts": {
        "scss": "node-sass --output-style compressed -o dist assets/css",
        "watch-scss": "node-sass --output-style compressed -wr -o dist assets/css",
        "dev": "npm run watch-scss",
        "prod": "npm run scss"
    }
}
